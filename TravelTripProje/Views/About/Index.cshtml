@using TravelTripProje.Models.Siniflar
@model List<Hakkimizda> 
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>About</h3>


            @foreach (var x in Model)
            {
                <p>@x.Aciklama</p>
                <img src="@x.FotoUrl" alt="" />

            }


        </div>
    </div>
</div>


